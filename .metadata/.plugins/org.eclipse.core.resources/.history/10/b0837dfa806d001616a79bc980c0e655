void QuickSort(int R[], int low, int high)
{
	int pivotpos;
	if(low < high)
	{
		pivotpos = Partition(R, low, high);
		QuickSort(R, low, pivotpos - 1);
		QuickSort(R, pivotpos + 1, high);
	}



}

int Partition(int R[], int i, int j)
{


}


